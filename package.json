{
  "name": "css-extract",
  "version": "1.3.0",
  "description": "Extract CSS from a browserify bundle",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "test": "standard && npm run deps && NODE_ENV=test node test",
    "test:cov": "standard && npm run deps && NODE_ENV=test istanbul cover ./test/index.js"
  },
  "repository": "stackcss/css-extract",
  "keywords": [
    "css",
    "extract",
    "browserify",
    "plugin",
    "transform",
    "sheetify",
    "css-modules"
  ],
  "license": "MIT",
  "dependencies": {
    "bl": "^4.0.2",
    "defined": "^1.0.0",
    "from2-string": "^1.1.0",
    "static-module": "^3.0.0",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "dependency-check": "^2.10.1",
    "insert-css": "^2.0.0",
    "istanbul": "^0.4.2",
    "sheetify": "^8.0.0",
    "standard": "^14.3.4",
    "tape": "^5.0.1",
    "tmp": "^0.2.1"
  },
  "files": [
    "index.js",
    "bin/*"
  ]
}
