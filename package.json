{
  "name": "css-extract",
  "version": "1.2.0",
  "description": "Extract CSS from a browserify bundle",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "test": "standard && npm run deps && NODE_ENV=test node test",
    "test:cov": "standard && npm run deps && NODE_ENV=test istanbul cover ./test/index.js"
  },
  "repository": "stackcss/css-extract",
  "keywords": [
    "css",
    "extract",
    "browserify",
    "plugin",
    "transform",
    "sheetify",
    "css-modules"
  ],
  "license": "MIT",
  "dependencies": {
    "bl": "^1.1.2",
    "defined": "^1.0.0",
    "from2-string": "^1.1.0",
    "static-module": "^2.2.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "dependency-check": "^2.5.1",
    "insert-css": "^2.0.0",
    "istanbul": "^0.4.2",
    "sheetify": "^6.0.0",
    "standard": "^6.0.7",
    "tape": "^4.5.0",
    "tmp": "0.0.28"
  },
  "files": [
    "index.js",
    "bin/*"
  ]
}
